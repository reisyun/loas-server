{
  "name": "loas-server",
  "version": "0.0.1",
  "main": "src/main.ts",
  "description": "Animation list service server",
  "author": "reisyun",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "postinstall": "yarn prisma:generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate save --experimental && prisma migrate up --experimental",
    "prisma:studio": "prisma studio --experimental",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/core": "^7.5.5",
    "@nestjs/cqrs": "^7.0.1",
    "@nestjs/graphql": "^7.9.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.4",
    "@nestjs/platform-express": "^7.5.5",
    "@prisma/client": "^2.12.1",
    "apollo-server-express": "^2.19.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "env-var": "^7.0.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.3",
    "@nestjs/schematics": "^7.2.2",
    "@nestjs/testing": "^7.5.5",
    "@prisma/cli": "^2.12.1",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.9",
    "@types/jest": "26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.10",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  }
}
